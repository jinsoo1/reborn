<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_account"
    app:startDestination="@id/heightFragment">

    <fragment
        android:id="@+id/heightFragment"
        android:name="com.reborn.reborn.ui.view.account.height.HeightFragment"
        android:label="HeightFragment"
        tools:layout="@layout/fragment_height">
        <action
            android:id="@+id/action_heightFragment_to_weightFragment"
            app:destination="@id/weightFragment" />
    </fragment>
    <fragment
        android:id="@+id/weightFragment"
        android:name="com.reborn.reborn.ui.view.account.weight.WeightFragment"
        android:label="WeightFragment"
        tools:layout="@layout/fragment_weight"
        >
        <action
            android:id="@+id/action_weightFragment_to_experienceFragment"
            app:destination="@id/experienceFragment" />
    </fragment>
    <fragment
        android:id="@+id/experienceFragment"
        android:name="com.reborn.reborn.ui.view.account.experience.ExperienceFragment"
        android:label="ExperienceFragment"
        tools:layout="@layout/fragment_experience">
        <action
            android:id="@+id/action_experienceFragment_to_termsFragment"
            app:destination="@id/termsFragment" />
    </fragment>
    <fragment
        android:id="@+id/termsFragment"
        android:name="com.reborn.reborn.ui.view.account.terms.TermsFragment"
        android:label="TermsFragment"
        tools:layout="@layout/fragment_terms"
        />
</navigation>